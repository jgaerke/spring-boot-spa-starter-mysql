    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "^3.3.0",
    "file-loader": "0.8.5",
    "mocha": "^3.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.5",
    "zombie": "^4.2.1"
